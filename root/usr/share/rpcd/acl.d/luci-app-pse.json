{
	"luci-app-pse": {
		"description": "Grant UCI access for luci-app-pse",
		"read": {
			"ubus": {
				"luci": [ "getNetworkDevices" ],
				"pse": [ "get_poe_status", "recovery", "toggle" ]
			},
			"uci": [ "pse" ],
			"file": {
				"/etc/init.d/pse-daemon restart": ["exec"]
			}
		},
		"write": {
			"uci": [ "pse" ]
		}
	}
}
